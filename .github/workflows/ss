name: ss

on:
  push:
    branches:
      - main
    workflow_dispatch:

jobs:
  substring:
    runs-on: ubuntu-latest
    steps:
      - name: Extract Substring
        id: extract
        run: |
          my_string="Hello World"
          my_substring="${my_string:6:5}" # Extracts substring starting from index 6 with a length of 5
          echo "::set-output name=substring::$my_substring"
          
      - name: Print Substring
        run: echo "Extracted Substring: ${{ steps.extract.outputs.substring }}"
